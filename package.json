{
  "name": "kiwitype",
  "version": "1.0.0",
  "description": "Typescript rewrite of KiwiBot",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier": "npx prettier --check ./src",
    "prettier:fix": "npx prettier --write ./src",
    "format:all": "npm run prettier:fix && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defnot001/KiwiType"
  },
  "author": "defnot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defnot001/KiwiType/issues"
  },
  "homepage": "https://github.com/defnot001/KiwiType#readme",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "discord.js": "^14.5.0",
    "dotenv": "^16.0.2"
  }
}
